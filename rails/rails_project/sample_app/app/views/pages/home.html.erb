<%= provide(:title, @title) %>
<h1><%= @title %></h1>
<% if signed_in? %>
  <div class="row">
    <aside class="span3">
      <section>
        <%= render 'shared/my_info' %>
      </section>
    </aside>
    <span class="span9">
     <section>
        <%= render 'shared/micropost_form' %>
     </section>
     <%= render 'shared/feed' %> 
    </span>
  </div>
<% else %>
  <p>
  <%= link_to "Sign Up Now!", signup_path, :class => "btn btn-large btn-primary"%>
  <br>
<% end%>

