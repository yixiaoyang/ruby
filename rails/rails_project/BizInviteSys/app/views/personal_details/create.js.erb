<%if @personal_detail.nil? %>
<% err_msg ||= 'Create Failed' %>
  alert(<%=err_msg%>);
<%else%>
  $('#personal_detail_list table tbody').append("<%= escape_javascript(render(@personal_detail, :ajax_view => true)) %>");
  $('#personal_detail_new_ajax').remove();
  $("#<%=PersonalDetailsHelper.gen_tr_id(@personal_detail.id)%>").fadeIn(400).fadeOut(400).fadeIn(200).fadeOut(200).fadeIn("slow");
<%end%>