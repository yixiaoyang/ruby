<% provide(:title, "Education")%>
<p id="notice"><%= notice %></p>

<div class="row">
  <aside class="span3">
  <div class="aside-left-my-info">
    <%= render 'shared/my_info' %>
  </div>
  </aside>
  
  <div class="span9 ">
    <ul class="normal_ul">
    <table class="table table-hover table-striped table-bordered">
      <%= render @education%>
    </table>
    
    <%= link_to 'Edit', edit_education_path(@education) %> |
    <%= link_to 'Back', educations_path %>
    </ul>
  </div>
</div>
