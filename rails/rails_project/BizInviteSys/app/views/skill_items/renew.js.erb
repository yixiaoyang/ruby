<%if @error.nil? %>
  var tab_div = $('#skill_items_list table tbody');
  tab_div.empty();
  
  <% (0...SKILL_CATEGORY_WORDS.size).each {|idx|  %>
    tab_div.append("<%= escape_javascript(render 'profiles/skill_for_profile', :skills =>  @profile.skills.where(:category => idx).order("id ASC"), :category => idx) %>");
  <% } %>
  
  $('#skill_slt_modal').modal('hide');  
  $('#skill_item_table_title').html("Skills( <%=@profile.skill_items.count %> )");
<% else %>
  alert("Save Failed:<%=@error%>");
<%end%>