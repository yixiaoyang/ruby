<% provide(:title, "Listing skill_itemss")%>
<h1>Listing skill_items</h1>

<div class="row">
  <aside class="span3">
  <div class="aside-left-my-info">
    <%= render 'shared/my_info' %>
  </div>
  </aside>
  
  <div class="span9 ">
    <%= will_paginate %>
    <ul class="normal_ul">
    <table class="table table-hover table-striped table-bordered">
      <%= render @skill_items %>
    </table>
    </ul>
    <%= will_paginate %>
    
    <br>
    <%= link_to 'New Skill item', new_skill_item_path %>
  </div>
</div>
<br>
